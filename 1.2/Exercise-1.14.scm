                  (cc 11 3)                (cc 11 2)                   (cc 11 2)      (cc 11 1)                  (cc 11 2)    (cc 11 1)    (cc 11 1)      (cc 11 0)
    
            (cc 11 2)    (cc 11 1)     (cc 11 1)(cc 11 0)           (cc 11 1)(cc 11 0)                        (cc 11 1)(cc 11 0)
    
